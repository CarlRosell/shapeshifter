{
  "name": "reagent",
  "version": "0.0.1",
  "description": "Generate resource schemas for React PropTypes, Flow, and TypeScript.",
  "main": "reagent.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "babel ./src -d ./lib",
    "watch": "babel --watch ./src -d ./lib",
    "lint": "npm run lint:code && npm run lint:flow",
    "lint:code": "eslint ./src ./tests",
    "lint:flow": "flow check ./tests/expected/flow",
    "mocha": "mocha --opts ./tests/mocha.opts ./tests/*.test.js ./tests/**/*.test.js",
    "test": "npm run lint && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/reagent.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/reagent/issues"
  },
  "homepage": "https://github.com/milesj/reagent#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "command-line-args": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-files": "^1.2.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-react": "^5.1.1",
    "flow-bin": "^0.27.0",
    "mocha": "^2.5.3",
    "react": "^15.1.0"
  }
}
