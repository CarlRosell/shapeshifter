{
  "name": "shapeshifter",
  "version": "1.2.0",
  "description": "Generate React prop types, Flow types, or TypeScript interfaces from JSON schema files.",
  "keywords": [
    "generate",
    "schema",
    "react",
    "prop",
    "flow",
    "typescript",
    "type",
    "alias",
    "definitions",
    "interface",
    "struct",
    "shape",
    "shift",
    "shifter",
    "transpile",
    "entity"
  ],
  "bin": {
    "shapeshifter": "./bin/shapeshifter.js"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "babel ./src -d ./lib",
    "watch": "babel --watch ./src -d ./lib",
    "lint": "eslint ./src ./tests",
    "check": "npm run check:flow && npm run check:typescript",
    "check:flow": "flow check ./tests/expected/flow",
    "check:typescript": "tsc --noEmit --allowJs ./tests/expected/typescript/*",
    "mocha": "mocha --opts ./tests/mocha.opts ./tests/*.test.js ./tests/**/*.test.js",
    "test": "npm run build && npm run lint && npm run mocha && npm run check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/shapeshifter.git"
  },
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/shapeshifter/issues"
  },
  "homepage": "https://github.com/milesj/shapeshifter#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "command-line-args": "^3.0.1",
    "command-line-usage": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chai-files": "^1.4.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-flowtype": "^2.11.4",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "flow-bin": "^0.31.1",
    "mocha": "^3.0.2",
    "react": "^15.3.1",
    "typescript": "^1.8.10"
  }
}
