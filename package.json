{
  "name": "shapeshifter",
  "version": "2.3.0",
  "description": "Generate relational schemas, React prop types, Flow type aliases, or TypeScript interfaces from JSON schema files.",
  "keywords": [
    "generate",
    "schema",
    "react",
    "prop",
    "flow",
    "typescript",
    "type",
    "alias",
    "definitions",
    "interface",
    "struct",
    "shape",
    "shift",
    "shifter",
    "transpile",
    "entity"
  ],
  "main": "./lib/index.js",
  "bin": "./bin/shapeshifter.js",
  "files": [
    "*.md",
    "bin",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "babel ./src -d ./lib && cp ./flow/shapeshifter.js.flow ./lib",
    "lint": "eslint ./src ./tests",
    "check": "yarn run check:flow && yarn run check:typescript",
    "check:flow": "flow check",
    "check:typescript": "tsc --noEmit --allowJs ./tests/expected/typescript/*",
    "mocha": "mocha --opts ./mocha.opts './tests/**/*.test.js'",
    "pretest": "yarn run lint",
    "test": "yarn run mocha",
    "posttest": "yarn run check",
    "preversion": "yarn test && yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/shapeshifter.git"
  },
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/shapeshifter/issues"
  },
  "homepage": "https://github.com/milesj/shapeshifter#readme",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "chalk": "^1.1.0",
    "command-line-args": "^3.0.0",
    "command-line-usage": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-comments": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-files": "^1.4.0",
    "eslint": "^3.12.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.37.0",
    "mocha": "^3.1.2",
    "react": "^15.4.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.1.4"
  }
}
